# -*- mode: snippet -*-
# name: Stateful Widget
# key: stfw_
# --

class MyStatefulWidget extends StatefulWidget {
  @override
  _State createState() => new _State();
}

class _State extends State<MyStatefulWidget> {
  @override
  Widget build(BuildContext context) {
    return new Scaffold(
      appBar: new AppBar(
        title: new Text('My Title')
      )
    );
  }
}
